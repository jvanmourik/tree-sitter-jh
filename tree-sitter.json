{
  "grammars": [
    {
      "name": "jh",
      "camelcase": "JH",
      "scope": "source.jh",
      "path": ".",
      "file-types": [
        "jh"
      ],
      "highlights": [
        "queries/highlights.scm",
        "queries/highlights-jsx.scm",
        "queries/highlights-params.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ],
      "injection-regex": "^(js|javascript)$"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "JavaScript-HTML grammar for tree-sitter",
    "authors": [
      {
        "name": "Max Brunsfeld",
        "email": "maxbrunsfeld@gmail.com"
      },
      {
        "name": "Amaan Qureshi",
        "email": "amaanq12@gmail.com"
      },
      {
        "name": "Jacco van Mourik"
      }
    ],
    "links": {
      "repository": "https://github.com/jvanmourik/tree-sitter-jh"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
